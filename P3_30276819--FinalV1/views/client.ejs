    <!-- Portfolio Start -->
    <div class="wrapper" id="our_products">
        <h1 class="display-5 text-uppercase mb-4 text-white text-center"> Nuestros Productos </h1>
        <form action="/" method="get" class="dark-form">
            <input type="text" name="nombre" placeholder="Nombre">
            <input type="number" name="precio" placeholder="Precio">
            <input type="text" name="image" placeholder="Imagen">
            <button type="submit">Filtrar</button>
        </form>
        <div class="links">
            <ul>
                <li data-view="list-view" class="li-grid">
                    <i class="fas fa-th-list"></i>
                    Modo Lista
                </li>
                <li data-view="grid-view" class="li-grid">
                    <i class="fas fa-th-large"></i>
                    Vista en cuadrícula
                </li>
            </ul>
        </div>
        <div class="view_main">
            <div class="view_wrap list-view">
                <% products.forEach(function(product) { %>
                    <div class="view_item">
                        <div class="vi_left">
                            <img src="/<%= product.image.slice(8) %>" alt="<%= product.name %>">
                        </div>
                        <div class="vi_right">
                            <p class="title"><%= product.nombre %></p>
                            <p class="content"><%= product.precio %> $ </p>                            
                            <a href="/product_detail/<%= product.codigo %>" class="btn">Ver más</a>
                        </div>
                    </div>
                <% }); %>
            </div>
            <button data-view="grid-view" class="btn-grid btn btn-warning btn-lg">
                <img width="64" height="64" src="https://img.icons8.com/glyph-neue/64/buy--v1.png" alt="buy--v1"/>
                Comprar ahora!
            </button>
            <div class="view_wrap grid-view" style="display: none;">
                <% products.forEach(function(product) { %>
                    <div class="view_item">
                        <div class="vi_left">
                            <img src="/<%= product.image.slice(8) %>" class="product-image" alt="<%= product.name %>">
                        </div>
                        <div class="vi_right">
                            <p class="title"><%= product.nombre %></p>
                            <p class="content"><%= product.precio %> $ </p>
                            <div id="rateYo"></div>
                            <span class="text-dark">Rating: 0</span>
                            <a href="/product_detail/<%= product.codigo %>" class="btn">Ver más</a>
                            <button class="buy-button">
                                <img width="94" height="94" src="https://img.icons8.com/3d-fluency/94/shopping-cart.png" alt="shopping-cart"/>
                                Comprar
                            </button>
                        </div>
                        <input type="hidden" value="<%= product.nombre %>" class="hiddenProductName">
                        <input type="hidden" value="<%= product.precio %>" class="hiddenProductDescription">
                        <input type="hidden" value="<%= product.image %>" class="hiddenProductImage">
                    </div>
                <% }); %>
            </div>
        </div>
    </div>
    <div class="shopping-list">
        <h3>Lista de Compras </h3>       
        <ul class="cart-items"></ul>
        <p class="text-dark total-price h3 font-weight-bold">Total: $0</p>
        <p class="text-dark cantidad h3 font-weight-bold" id="cantidad">Cantidad: 0</p>
        <button id="pagar" type="button" class="btn btn-primary btn-lg btn-block btn-pagar">
            Pagar
        </button>        
    </div>

    
                    

